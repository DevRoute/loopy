{
  "name": "loopy-programming-guide",
  "type": "module",
  "version": "0.0.1",
  "description": "Loopy Programming Guide",
  "author": "Moment",
  "license": "MIT",
  "homepage": "https://github.com/pdsuwwz/nextjs-nextra-starter#readme",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/pdsuwwz/nextjs-nextra-starter.git"
  },
  "bugs": {
    "url": "https://github.com/pdsuwwz/nextjs-nextra-starter/issues"
  },
  "scripts": {
    "dev": "next dev --turbopack -p 8000",
    "build": "next build && next-sitemap",
    "postbuild": "pagefind --site .next/server/app --output-path public/_pagefind",
    "start": "next start -p 7000",
    "lint": "eslint \"**/*.{ts,tsx,js,jsx}\" --fix",
    "lint:ci": "eslint \"**/*.{ts,tsx,js,jsx}\"",
    "format": "prettier --write \"**/*.{ts,tsx,js,jsx,cjs,mjs,json}\"",
    "format:ci": "prettier --list-different \"**/*.{ts,tsx,js,jsx}\"",
    "postinstall": "husky install"
  },
  "dependencies": {
    "@emotion/react": "^11.14.0",
    "@emotion/styled": "^11.14.0",
    "@fingerprintjs/fingerprintjs": "^4.6.0",
    "@radix-ui/react-accordion": "^1.2.3",
    "@radix-ui/react-hover-card": "^1.1.6",
    "@radix-ui/react-separator": "^1.1.2",
    "@radix-ui/react-slot": "^1.1.2",
    "@radix-ui/react-toggle": "^1.1.2",
    "@tsparticles/engine": "^3.8.1",
    "@tsparticles/react": "^3.0.0",
    "axios": "^1.8.1",
    "class-variance-authority": "^0.7.1",
    "clsx": "^2.1.1",
    "framer-motion": "^12.4.2",
    "lucide-react": "^0.475.0",
    "motion": "^12.4.2",
    "mysql2": "^3.12.0",
    "next": "15.1.7",
    "next-sitemap": "^4.2.3",
    "next-themes": "^0.4.4",
    "nextra": "^4.2.9",
    "nextra-theme-docs": "4.2.9",
    "qss": "^3.0.0",
    "react": "^19.0.0",
    "react-dom": "^19.0.0",
    "react-fast-marquee": "^1.6.5",
    "tailwind-merge": "^3.0.1",
    "tailwindcss-animate": "^1.0.7",
    "tsparticles": "^3.8.1"
  },
  "devDependencies": {
    "@iconify/json": "^2.2.305",
    "@iconify/tailwind4": "^1.0.3",
    "@svgr/core": "^8.1.0",
    "@svgr/plugin-jsx": "^8.1.0",
    "@svgr/plugin-svgo": "^8.1.0",
    "@svgr/webpack": "^8.1.0",
    "@tailwindcss/postcss": "^4.0.6",
    "@types/node": "^22.13.2",
    "@types/react": "^19.0.8",
    "@types/react-dom": "^19.0.3",
    "@typescript-eslint/eslint-plugin": "^8.25.0",
    "@typescript-eslint/parser": "^8.25.0",
    "commitizen": "^4.3.1",
    "cz-git": "^1.10.1",
    "eslint": "^9.12.0",
    "eslint-config-prettier": "^9.1.0",
    "eslint-plugin-import": "^2.31.0",
    "eslint-plugin-jsx-a11y": "^6.10.0",
    "eslint-plugin-next": "^0.0.0",
    "eslint-plugin-prettier": "^5.2.1",
    "eslint-plugin-react": "^7.37.1",
    "eslint-plugin-react-hooks": "^5.0.0",
    "eslint-plugin-storybook": "^0.9.0",
    "eslint-plugin-tailwindcss": "^3.17.5",
    "https-proxy-agent": "^7.0.6",
    "husky": "^9.1.6",
    "imagekit": "^6.0.0",
    "lint-staged": "^15.2.10",
    "node-fetch": "^3.3.2",
    "pagefind": "^1.3.0",
    "postcss": "^8.5.2",
    "prettier": "^3.5.2",
    "react-responsive": "^10.0.0",
    "sass": "^1.84.0",
    "sharp": "^0.33.5",
    "tailwindcss": "^4.0.6",
    "typescript": "^5.7.3"
  },
  "lint-staged": {
    "*.{ts,tsx,js,jsx,cjs,mjs,json}": [
      "pnpm lint",
      "pnpm format"
    ],
    "*.md{,x}": "node scripts/replace-image-urls.mjs ./content --force"
  },
  "config": {
    "commitizen": {
      "path": "node_modules/cz-git"
    }
  }
}
